A,CAAA,KAAA,GAAA,CAAA,CAAA,KAAA;;AACA,UAAA,CAAA,CAAA,KAAA,IAAA,QAAA,EAAA;AACA,aAAA,IAAA,CAAA,KAAA,GAAA,EAAA;AACA;;AACA,WAAA,WAAA;AACA,KArDA;AAsDA,IAAA,MAtDA,kBAsDA,CAtDA,EAsDA;AAAA;;AACA,WAAA,MAAA,CAAA,OAAA,CAAA;AACA,QAAA,KAAA,EAAA,QADA;AAEA;AACA,QAAA,OAAA,EAAA,gBAHA;AAIA,QAAA,OAAA,EAAA,IAJA;AAKA,QAAA,IAAA,EAAA,gBAAA;AACA;AACA,cAAA,MAAA,GAAA;AACA,YAAA,GAAA,EAAA,CAAA,CAAA,EADA;AAEA,YAAA,SAAA,EAAA,CAAA,CAAA,cAFA;AAGA,YAAA,UAAA,EAAA,MAAA,CAAA,IAAA,CAAA;AAHA,WAAA;AAKA,UAAA,QAAA,CAAA,MAAA,CAAA,CAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,YAAA,MAAA,CAAA,MAAA,CAAA,MAAA;;AACA,gBAAA,GAAA,CAAA,OAAA,EAAA;AACA,cAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AACA,cAAA,MAAA,CAAA,WAAA;AACA;AACA,WANA;AAOA;AAnBA,OAAA;AAqBA,KA5EA;AA6EA,IAAA,UA7EA,sBA6EA,CA7EA,EA6EA;AACA,WAAA,IAAA,CAAA,UAAA,GAAA,CAAA;AACA,WAAA,IAAA,CAAA,WAAA,GAAA,CAAA,CAAA,MAAA;AACA,KAhFA;AAiFA,IAAA,cAjFA,4BAiFA;AACA,WAAA,KAAA,CAAA,KAAA,CAAA,SAAA,CAAA,KAAA;AACA,KAnFA;AAoFA,IAAA,MApFA,oBAoFA;AAAA;;AACA,UAAA,KAAA,IAAA,CAAA,WAAA,IAAA,CAAA,EAAA;AACA,aAAA,QAAA,CAAA,OAAA,CAAA,aAAA;AACA;AACA;;AACA,WAAA,MAAA,CAAA,OAAA,CAAA;AACA,QAAA,KAAA,EAAA,QADA;AAEA,QAAA,OAAA,EAAA,iBAAA,KAAA,IAAA,CAAA,WAAA,GAAA,OAFA;AAGA,QAAA,OAAA,EAAA,IAHA;AAIA,QAAA,IAAA,EAAA,gBAAA;AACA,cAAA,GAAA,GAAA,EAAA;;AACA,UAAA,MAAA,CAAA,IAAA,CAAA,UAAA,CAAA,OAAA,CAAA,UAAA,CAAA,EAAA;AACA,YAAA,GAAA,IAAA,CAAA,CAAA,EAAA,GAAA,GAAA;AACA,WAFA;;AAGA,UAAA,GAAA,GAAA,GAAA,CAAA,SAAA,CAAA,CAAA,EAAA,GAAA,CAAA,MAAA,GAAA,CAAA,CAAA;AACA,cAAA,SAAA,GAAA,EAAA;;AACA,UAAA,MAAA,CAAA,IAAA,CAAA,UAAA,CAAA,OAAA,CAAA,UAAA,CAAA,EAAA;AACA,YAAA,SAAA,IAAA,CAAA,CAAA,cAAA,GAAA,GAAA;AACA,WAFA;;AAGA,UAAA,SAAA,GAAA,SAAA,CAAA,SAAA,CAAA,CAAA,EAAA,GAAA,CAAA,MAAA,GAAA,CAAA,CAAA;AACA,cAAA,MAAA,GAAA;AACA,YAAA,GAAA,EAAA,GADA;AAEA,YAAA,SAAA,EAAA,SAFA;AAGA,YAAA,UAAA,EAAA,MAAA,CAAA,IAAA,CAAA;AAHA,WAAA;AAKA,UAAA,QAAA,CAAA,MAAA,CAAA,CAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,YAAA,MAAA,CAAA,MAAA,CAAA,MAAA;;AACA,gBAAA,GAAA,CAAA,OAAA,EAAA;AACA,cAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AACA,cAAA,MAAA,CAAA,cAAA;;AACA,cAAA,MAAA,CAAA,WAAA;AACA;AACA,WAPA;AAQA;AA5BA,OAAA;AA8BA;AAvHA,GApKA;AA6RA,EAAA,OA7RA,qBA6RA;AACA,SAAA,IAAA;AACA;AA/RA,CAAA","sourcesContent":["<style lang=\"less\">\n@import \"../../../styles/table-common.less\";\n@import \"./socialManage.less\";\n</style>\n<template>\n  <div class=\"search\">\n    <Card>\n      <Tabs v-model=\"tabName\" :animated=\"false\" @on-click=\"changeTab\">\n        <TabPane label=\"Github\" name=\"0\"></TabPane>\n        <TabPane label=\"QQ\" name=\"1\"></TabPane>\n        <TabPane label=\"微信\" name=\"3\"></TabPane>\n        <TabPane label=\"微博\" name=\"2\"></TabPane>\n      </Tabs>\n      <Row v-show=\"openSearch\" @keydown.enter.native=\"handleSearch\">\n        <Form ref=\"form\" :model=\"form\" inline :label-width=\"100\">\n          <Form-item label=\"社交账号昵称\" prop=\"username\">\n            <Input\n              type=\"text\"\n              v-model=\"form.username\"\n              placeholder=\"请输入第三方社交账号昵称\"\n              clearable\n              style=\"width: 200px\"\n            />\n          </Form-item>\n          <Form-item label=\"绑定用户名\" prop=\"relateUsername\">\n            <Input\n              type=\"text\"\n              v-model=\"form.relateUsername\"\n              placeholder=\"请输入绑定系统用户名\"\n              clearable\n              style=\"width: 200px\"\n            />\n          </Form-item>\n          <Form-item label=\"创建时间\">\n            <DatePicker\n              v-model=\"selectDate\"\n              type=\"daterange\"\n              format=\"yyyy-MM-dd\"\n              clearable\n              @on-change=\"selectDateRange\"\n              placeholder=\"选择起始时间\"\n              style=\"width: 200px\"\n            ></DatePicker>\n          </Form-item>\n          <Form-item style=\"margin-left:-35px;\" class=\"br\">\n            <Button @click=\"handleSearch\" type=\"primary\" icon=\"ios-search\">搜索</Button>\n            <Button @click=\"handleReset\">重置</Button>\n          </Form-item>\n        </Form>\n      </Row>\n      <Row class=\"operation\">\n        <Button @click=\"delAll\" icon=\"md-trash\">批量删除解绑</Button>\n        <Button @click=\"getDataList\" icon=\"md-refresh\">刷新</Button>\n        <Button type=\"dashed\" @click=\"openSearch=!openSearch\">{{openSearch ? \"关闭搜索\" : \"开启搜索\"}}</Button>\n        <Button type=\"dashed\" @click=\"openTip=!openTip\">{{openTip ? \"关闭提示\" : \"开启提示\"}}</Button>\n      </Row>\n      <Row v-show=\"openTip\">\n        <Alert show-icon>\n          已选择\n          <span class=\"select-count\">{{selectCount}}</span> 项\n          <a class=\"select-clear\" @click=\"clearSelectAll\">清空</a>\n        </Alert>\n      </Row>\n      <Row>\n        <Table\n          :loading=\"loading\"\n          border\n          :columns=\"columns\"\n          :data=\"data\"\n          sortable=\"custom\"\n          @on-sort-change=\"changeSort\"\n          @on-selection-change=\"showSelect\"\n          ref=\"table\"\n        ></Table>\n      </Row>\n      <Row type=\"flex\" justify=\"end\" class=\"page\">\n        <Page\n          :current=\"pageNumber\"\n          :total=\"total\"\n          :page-size=\"pageSize\"\n          @on-change=\"changePage\"\n          @on-page-size-change=\"changePageSize\"\n          :page-size-opts=\"[10,20,50]\"\n          size=\"small\"\n          show-total\n          show-elevator\n          show-sizer\n        ></Page>\n      </Row>\n    </Card>\n  </div>\n</template>\n\n<script>\nimport { getRelatedListData, unRelate } from \"@/api/index\";\nexport default {\n  name: \"social-manage\",\n  data() {\n    return {\n      openSearch: true,\n      openTip: true,\n      tabName: \"0\",\n      data: [],\n      total: 0,\n      selectDate: null,\n      loading: true, // 表单加载状态\n      selectCount: 0, // 多选计数\n      selectList: [], // 多选数据\n      form: {\n        // 搜索框对应data对象\n        socialType: \"0\",\n        username: \"\",\n        relateUsername: \"\",\n        pageNumber: 1, // 当前页数\n        pageSize: 10, // 页面大小\n        sort: \"createTime\", // 默认排序字段\n        order: \"desc\", // 默认排序方式\n        startDate: \"\", // 起始时间\n        endDate: \"\" // 终止时间\n      },\n      columns: [\n        // 表头\n        {\n          type: \"selection\",\n          width: 60,\n          align: \"center\",\n          fixed: \"left\"\n        },\n        {\n          type: \"index\",\n          width: 60,\n          align: \"center\",\n          fixed: \"left\"\n        },\n        {\n          title: \"社交账号昵称\",\n          key: \"username\",\n          minWidth: 180,\n          sortable: true,\n          fixed: \"left\",\n          render: (h, params) => {\n            if (\n              this.$route.meta.permTypes &&\n              this.$route.meta.permTypes.includes(\"view\")\n            ) {\n              return h(\"span\", params.row.username);\n            } else {\n              return h(\"span\", \"你无权查看该数据\");\n            }\n          }\n        },\n        {\n          title: \"头像\",\n          key: \"avatar\",\n          width: 80,\n          align: \"center\",\n          render: (h, params) => {\n            return h(\"Avatar\", {\n              props: {\n                src: params.row.avatar\n              }\n            });\n          }\n        },\n        {\n          title: \"openID\",\n          key: \"openId\",\n          width: 200,\n          sortable: true,\n          render: (h, params) => {\n            if (\n              this.$route.meta.permTypes &&\n              this.$route.meta.permTypes.includes(\"view\")\n            ) {\n              return h(\"span\", params.row.openId);\n            } else {\n              return h(\"span\", \"你无权查看该数据\");\n            }\n          }\n        },\n        {\n          title: \"是否绑定用户\",\n          key: \"isRelated\",\n          width: 150,\n          align: \"center\",\n          sortable: true,\n          render: (h, params) => {\n            if (params.row.isRelated) {\n              return h(\"div\", [\n                h(\n                  \"Tag\",\n                  {\n                    props: {\n                      color: \"blue\"\n                    }\n                  },\n                  \"已绑定\"\n                )\n              ]);\n            } else {\n              return h(\"div\", [\n                h(\n                  \"Tag\",\n                  {\n                    props: {\n                      color: \"default\"\n                    }\n                  },\n                  \"未绑定\"\n                )\n              ]);\n            }\n          }\n        },\n        {\n          title: \"绑定用户\",\n          key: \"relateUsername\",\n          width: 200,\n          sortable: true\n        },\n        {\n          title: \"创建时间\",\n          key: \"createTime\",\n          width: 170,\n          sortable: true,\n          sortType: \"desc\"\n        },\n        {\n          title: \"操作\",\n          key: \"action\",\n          width: 180,\n          align: \"center\",\n          fixed: \"right\",\n          render: (h, params) => {\n            return h(\"div\", [\n              h(\n                \"Button\",\n                {\n                  props: {\n                    icon: \"md-link\",\n                    type: \"error\",\n                    size: \"small\"\n                  },\n                  on: {\n                    click: () => {\n                      this.remove(params.row);\n                    }\n                  }\n                },\n                \"删除解绑\"\n              )\n            ]);\n          }\n        }\n      ],\n      data: [], // 表单数据\n      total: 0 // 表单数据总数\n    };\n  },\n  methods: {\n    init() {\n      this.getDataList();\n    },\n    changeTab(v) {\n      this.form.socialType = v;\n      this.getDataList();\n    },\n    changePage(v) {\n      this.form.pageNumber = v;\n      this.getDataList();\n      this.clearSelectAll();\n    },\n    changePageSize(v) {\n      this.tabName.pageSize = v;\n      this.getDataList();\n    },\n    selectDateRange(v) {\n      this.form.startDate = v[0];\n      this.form.endDate = v[1];\n    },\n    getDataList() {\n      this.loading = true;\n      getRelatedListData(this.form).then(res => {\n        if (res.success) {\n          this.loading = false;\n          this.data = res.result.content;\n          this.total = res.result.totalElements;\n        }\n      });\n    },\n    handleSearch() {\n      this.form.pageNumber = 1;\n      this.form.pageSize = 10;\n      this.getDataList();\n    },\n    handleReset() {\n      this.$refs.form.resetFields();\n      this.form.pageNumber = 1;\n      this.form.pageSize = 10;\n      this.selectDate = null;\n      this.form.startDate = \"\";\n      this.form.endDate = \"\";\n      // 重新加载数据\n      this.getDataList();\n    },\n    changeSort(e) {\n      this.form.sort = e.key;\n      this.form.order = e.order;\n      if (e.order == \"normal\") {\n        this.form.order = \"\";\n      }\n      this.getDataList();\n    },\n    remove(v) {\n      this.$Modal.confirm({\n        title: \"确认删除解绑\",\n        // 记得确认修改此处\n        content: \"您确认要删除解绑该条数据 ?\",\n        loading: true,\n        onOk: () => {\n          // 删除\n          let params = {\n            ids: v.id,\n            usernames: v.relateUsername,\n            socialType: this.form.socialType\n          };\n          unRelate(params).then(res => {\n            this.$Modal.remove();\n            if (res.success) {\n              this.$Message.success(\"操作成功\");\n              this.getDataList();\n            }\n          });\n        }\n      });\n    },\n    showSelect(e) {\n      this.form.selectList = e;\n      this.form.selectCount = e.length;\n    },\n    clearSelectAll() {\n      this.$refs.table.selectAll(false);\n    },\n    delAll() {\n      if (this.form.selectCount <= 0) {\n        this.$Message.warning(\"您还未选择要删除的数据\");\n        return;\n      }\n      this.$Modal.confirm({\n        title: \"确认删除解绑\",\n        content: \"您确认要删除解绑所选的 \" + this.form.selectCount + \" 条数据?\",\n        loading: true,\n        onOk: () => {\n          let ids = \"\";\n          this.form.selectList.forEach(function(e) {\n            ids += e.id + \",\";\n          });\n          ids = ids.substring(0, ids.length - 1);\n          let usernames = \"\";\n          this.form.selectList.forEach(function(e) {\n            usernames += e.relateUsername + \",\";\n          });\n          usernames = usernames.substring(0, ids.length - 1);\n          let params = {\n            ids: ids,\n            usernames: usernames,\n            socialType: this.form.socialType\n          };\n          unRelate(params).then(res => {\n            this.$Modal.remove();\n            if (res.success) {\n              this.$Message.success(\"操作成功\");\n              this.clearSelectAll();\n              this.getDataList();\n            }\n          });\n        }\n      });\n    }\n  },\n  mounted() {\n    this.init();\n  }\n};\n</script>"],"sourceRoot":"src/views/sys/social-manage"}]}